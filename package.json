{
  "name": "okxos",
  "version": "0.0.17",
  "description": "okxos SDK",
  "homepage": "https://www.okx.com/web3/build/docs/waas/okx-waas-what-is-waas",
  "repository": {
    "url": "https://github.com/SOVLOOKUP/okxos"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "blockchain",
    "okx"
  ],
  "author": "sovlookup",
  "peerDependencies": {
    "zod": "^3.23.8"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "ethers": "^6.13.4",
    "ky": "^1.7.3",
    "qs": "^6.13.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/qs": "^6.9.17",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "clean": true,
    "dts": true,
    "treeshake": true,
    "shims": true,
    "minify": true
  },
  "files": [
    "dist/*"
  ]
}
